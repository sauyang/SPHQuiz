    package="com.sauyang.androidsample">

    <application
        android:name=".ui.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.register.PreloginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".module.register.MainMenuActivity"/>
        <activity android:name=".module.register.RegisterListActivity"/>
        <activity android:name=".module.register.EditActivity"/>
        <activity android:name=".module.register.RegisterActivity"/>
        <activity android:name=".module.graph.GraphDataActivity"/>
    </application>

